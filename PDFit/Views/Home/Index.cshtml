@{
    ViewData["Title"] = "Home Page";
}


<div class="row">
    <div class="col">
        <div class="card">
            <div class="card-header">
                PDF from HTML Text
            </div>
            <div class="card-body">
                <h5 class="card-title">https://pdfit.azurewebsites.net/GetPdfFromHTML</h5>

                <p>
                    <span class="text-muted">
                        host:
                    </span>
                    <br />
                    <span class="ml-2">
                        pdfit.azurewebsites.net
                    </span>
                </p>
                <p>
                    <span class="text-muted">
                        schemes:
                    </span>
                    <br />
                    <span class="ml-2">
                        https
                    </span>
                </p>
                <p>
                    <span class="text-muted">
                        paths:
                    </span>
                    <br />
                    <span class="ml-2">
                        GetPdfFromHTML
                    </span>
                </p>
                <p>
                    <span class="text-muted">
                        parameters:
                    </span>
                    <br />
                    <span class="ml-2">
                        - in : body
                    </span>
                </p>
                <p>
                    <span class="ml-3">
                        HTMLText
                    </span>                   
                    <span class="ml-3 font-italic">
                        html Text
                    </span>
                </p>

                <p class="card-text">Convert a HTML text to PDF Document</p>
                <p>
                    <script src="https://gist.github.com/naveenalavilli/f3314ad4c94d5f2c312e14057d717b40.js"></script>
                </p>
            </div>
        </div>
    </div>
</div>

<div class="row mt-2">
    <div class="col">
        <div class="card">
            <div class="card-header">
                PDF from Webpage URL
            </div>
            <div class="card-body">
                <h5 class="card-title">https://pdfit.azurewebsites.net/GetPDFfromURL</h5>
                <p class="card-text">Convert a webpage from URL to a PDF document.</p>
                <p>
                    <script src="https://gist.github.com/naveenalavilli/1a4d1974352f475716b4dad7aef906ba.js"></script>
                </p>
            </div>
        </div>
    </div>
